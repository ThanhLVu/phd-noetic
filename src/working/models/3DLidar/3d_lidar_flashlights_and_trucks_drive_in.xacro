            </geometry>
            <material name="Neobotix/Aluminum"/>
        </visual>
        <collision>
            <origin xyz="0.0149 0.0 0.0125" rpy="${M_PI/2} 0 ${M_PI/2}"/>
            <geometry>
                <box size="0.02505 0.09 0.025"/>
            </geometry>
        </collision>
        <inertial>
            <mass value="0.564" />
            <origin xyz="0.0149 0.0 0.0125" rpy="${M_PI/2} 0 ${M_PI/2}"/>
            <inertia ixx="0.003881243" ixy="0.0" ixz="0.0" iyy="0.000498940" iyz="0.0" izz="0.003879257" />
        </inertial>
    </link>
    
